{
	"modules": [
		{
			"module": {
				"name": "Homepage",
				"dependencies": [
					"Utils",
					"PrettyDate",
					"FavIcon",
					"OAuth",
					"Notifications",
					"Cache",
					"HomepageData"
				]
			},
			"resources": [
				"js/ng-modules/utils.js",
				"js/ng-filters/prettydate.js",
				"js/ng-filters/favicon.filter.js",
				"js/ng-services/homepage.data.js",
				"js/ng-services/oauth.js",
				"js/ng-services/storage.js",
				"js/ng-services/cache.js"

			],
			"widgets": [
				{ "name": "instagram" },
				{ "name": "google_reader" },
				{ "name": "rss" },
				{ "name": "slideshow" }
			]
		},
		{
			"module": { "name": "Notifications", "dependencies": ["OAuth", "Utils"] },
			"widgets": [
				{
					"name": "facebook",
					"settings": {
						"refreshRate": 15
					}
				},
				{
					"name": "gmail_notifications",
					"settings": {
						"refreshRate": 150
					}
				}
			]
		}
	]
}