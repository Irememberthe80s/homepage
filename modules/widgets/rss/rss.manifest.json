{
	"name": "RSS",
	"modules": [
		{ "name": "GoogleFeed", "dependencies": [] },
		{ "name": "Rss", "dependencies": [] }
	],
    "resources": [
		"google-feed.js",
		"rss.controller.js"
    ],
	"html": {
		"widget": "rss_reader.html"
	},
	"settingsOptions": [
		{
			"label": "Display mode",
			"name": "display",
			"type": "select",
			"options": [
                { "value": "headlines", "display": "Headlines" },
                { "value": "full", "display": "Full" },
                { "value": "gallery", "display": "Gallery" }
            ]
		},
		{
			"label": "Refresh rate",
			"name": "refreshRate",
			"type": "number",
			"appendix": " seconds",
			"min": 10,
			"max": 3600,
			"step": 5
		},
		{
			"label": "Feed URL",
			"name": "feed",
			"type": "text",
			"style": "long"
		}
	],
	"settings": {
		"display": "headlines",
		"refreshRate": 300
	}
}